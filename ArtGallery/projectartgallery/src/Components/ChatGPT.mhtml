">/* gallery-container styles */
.gallery-container {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 20px;
  }
 =20
  /* Filters section */
  .filters {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin-bottom: 20px;
    width: 80%;
  }
 =20
  .filters input, .filters select {
    padding: 10px;
    margin-right: 10px;
    width: 100%;
    max-width: 250px; /* Set a max width for inputs */
  }
 =20
  /* Gallery items container */
  .gallery-items {
    display: grid;
    grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));
    gap: 20px;
    width: 80%;
    margin-top: 20px;
  }
 =20
  /* Individual gallery item */
  .gallery-item {
    text-align: center;
    border: 1px solid #ddd;
    padding: 20px;
    background-color: #f9f9f9;
    transition: transform 0.3s ease-in-out;
    max-width: 250px; /* Limit width of each product item */
  }
 =20
  .gallery-item:hover {
    transform: scale(1.05); /* Slight zoom effect on hover */
  }
 =20
  /* Image styling */
  .gallery-item img {
    width: 100%; /* Ensure images scale with their container */
    height: auto;
    max-height: 200px; /* Set a max height for images to prevent them from =
becoming too large */
    object-fit: contain; /* Ensures the image fits within the box without d=
istortion */
  }
 =20
  /* Product name and description */
  h3 {
    font-size: 18px;
    font-weight: bold;
    margin-top: 10px;
  }
 =20
  p {
    margin: 5px 0;
    font-size: 14px;
  }
 =20
  /* Add to Cart button */
  button {
    background-color: #4CAF50; /* Green color */
    color: white;
    padding: 10px 20px;
    border: none;
    cursor: pointer;
    margin-top: 10px;
    font-size: 16px;
    border-radius: 5px;
    transition: background-color 0.3s ease, transform 0.2s ease;
  }
 =20
  button:hover {
    background-color: #45a049; /* Darker green on hover */
    transform: scale(1.05); /* Slight zoom effect on hover */
  }
 =20
  /* Cart section */
  .cart {
    margin-top: 40px;
    border-top: 2px solid #ddd;
    padding-top: 20px;
    width: 60%;
  }
 =20
  /* Cart item styles */
  .cart ul {
    list-style-type: none;
    padding: 0;
  }
 =20
  .cart li {
    margin-bottom: 10px;
  }
 =20
  .cart h3 {
    margin-top: 10px;
  }
